[
  {
    "name": "nginxdemos",
    "image": "nginxdemos/hello",
    "cpu": 256,
    "memory": 256,
    "essential": true,
    "portMappings": [
      {
        "containerPort": 80,
        "hostPort": 80,
        "protocol": "tcp"
      }
    ],
    "logConfiguration":{
        "logDriver":"awslogs",
        "options":{
            "awslogs-group":"awslogs-nginx-ecs",
            "awslogs-region":"us-east-1",
            "awslogs-stream-prefix":"ecs"
        }
    }
  }
]